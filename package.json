{
  "name": "ever-minute",
  "version": "0.0.1",
  "description": "To-Do on steroids",
  "productName": "Ever Minute",
  "author": "off-border <git@alpah.ru>",
  "private": true,
  "scripts": {
    "dev": "yarn --cwd ui quasar dev",
    "lint": "eslint --ext .js,.vue ./",
    "test:run": "vitest run --reporter=verbose",
    "test": "vitest --reporter=verbose",
    "test:core": "vitest --reporter=verbose --dir core",
    "test:ui": "vitest --reporter=verbose  --dir ui -c ui/vitest.config.js",
    "postinstall": "yarn --cwd ui && echo =====installed===== || ====== installation failed ======"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/vite-plugin": "^1.0.9",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/test-utils": "^2.0.0-rc.19",
    "quasar": "^2.6.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-babel": "^0.2.1",
    "eslint-config-standard": "^17.0.0-1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-n": "^14.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "jsdom": "^19.0.0",
    "vitest": "^0.8.1"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
