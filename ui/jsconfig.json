{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": [
        "src/*"
      ],
      "app/*": [
        "*"
      ],
      "components/*": [
        "src/ui/components/*"
      ],
      "layouts/*": [
        "src/ui/layouts/*"
      ],
      "pages/*": [
        "src/ui/pages/*"
      ],
      "assets/*": [
        "src/ui/assets/*"
      ],
      "boot/*": [
        "src/ui/boot/*"
      ],
      "stores/*": [
        "src/ui/stores/*"
      ],
      "vue$": [
        "node_modules/vue/dist/vue.runtime.esm-bundler.js"
      ]
    }
  },
  "exclude": [
    "dist",
    "src/ui/.quasar",
    ".vscode",
    "node_modules"
  ],
  "jsx": "preserve",
}
